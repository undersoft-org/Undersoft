{
  "Version": "0.1.1",
  "Title": "SSC Web Application Client",
  "Description": "Shared Service Center Web Application Client",
  "Repository": {
    "Clients": {
      "Undersoft.SSC.Infrastructure.Persistance.Services.OpenDataService": {
        "ConnectionString": "https://localhost:7200/",
        "ClientProvider": "Open",
        "PoolSize": 12
      }
    }
  },
  "DataCache": {
    "Hours": "0",
    "Minutes": "45"
  },
  "Serilog": {
    "MinimumLevel": "Verbose",
    "Using": [ "Serilog.Sinks.BrowserConsole" ],
    "WriteTo": [
      {
        "Name": "BrowserConsole"       
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
